ation.jsx\&quot;, \&quot;contents\&quot;: \&quot;import { useState } from \u0027react\u0027;\\nimport api from \u0027../utils/api\u0027;\\nimport toast from \u0027react-hot-toast\u0027;\\nimport { Upload, FileCheck, AlertCircle, CheckCircle } from \u0027lucide-react\u0027;\\n\\nexport default function Validation() {\\n  const [file, setFile] \u003d useState(null);\\n  const [uploading, setUploading] \u003d useState(false);\\n  const [jobId, setJobId] \u003d useState(null);\\n  const [report, setReport] \u003d useState(null);\\n\\n  const handleFileChange \u003d (e) \u003d\u003e {\\n    const selectedFile \u003d e.target.files[0];\\n    if (selectedFile) {\\n      const ext \u003d selectedFile.name.split(\u0027.\u0027).pop().toLowerCase();\\n      if ([\u0027csv\u0027, \u0027xlsx\u0027, \u0027xls\u0027].includes(ext)) {\\n        setFile(selectedFile);\\n        setReport(null);\\n        setJobId(null);\\n      } else {\\n        toast.error(\u0027Please select a CSV or Excel file\u0027);\\n      }\\n    }\\n  };\\n\\n  const handleUpload \u003d async () \u003d\u003e {\\n    if (!file) {\\n      toast.error(\u0027Please select a file\u0027);\\n      return;\\n    }\\n\\n    const formData \u003d new FormData();\\n    formData.append(\u0027file\u0027, file);\\n\\n    setUploading(true);\\n    try {\\n      const { data } \u003d await api.post(\u0027/validation/upload\u0027, formData, {\\n        headers: { \u0027Content-Type\u0027: \u0027multipart/form-data\u0027 },\\n      });\\n      \\n      setJobId(data.jobId);\\n      toast.success(\u0027File uploaded! Validation started...\u0027);\\n      \\n      // Poll for status\\n      pollStatus(data.jobId);\\n    } catch (error) {\\n      toast.error(error.response?.data?.error?.message || \u0027Upload failed\u0027);\\n      setUploading(false);\\n    }\\n  };\\n\\n  const pollStatus \u003d async (id) \u003d\u003e {\\n    const interval \u003d setInterval(async () \u003d\u003e {\\n      try {\\n        const { data } \u003d await api.get(`/validation/status/${id}`);\\n        \\n        if (data.job.status \u003d\u003d\u003d \u0027completed\u0027 || data.job.status \u003d\u003d\u003d \u0027failed\u0027) {\\n          clearInterval(interval);\\n          setUploading(false);\\n          \\n          if (data.job.status \u003d\u003d\u003d \u0027completed\u0027) {\\n            toast.success(\u0027Validation completed!\u0027);\\n            fetchReport(id);\\n          } else {\\n            toast.error(\u0027Validation failed\u0027);\\n          }\\n        }\\n      } catch (error) {\\n        clearInterval(interval);\\n        setUploading(false);\\n        toast.error(\u0027Failed to check status\u0027);\\n      }\\n    }, 2000);\\n  };\\n\\n  const fetchReport \u003d async (id) \u003d\u003e {\\n    try {\\n      const { data } \u003d await api.get(`/validation/report/${id}`);\\n      setReport(data);\\n    } catch (error) {\\n      toast.error(\u0027Failed to load report\u0027);\\n    }\\n  };\\n\\n  return (\\n    \u003cdiv className\u003d\\\&quot;p-8\\\&quot;\u003e\\n      \u003cdiv className\u003d\\\&quot;mb-8\\\&quot;\u003e\\n        \u003ch1 className\u003d\\\&quot;text-3xl font-bold text-gray-900\\\&quot;\u003eData Validation\u003c/h1\u003e\\n        \u003cp className\u003d\\\&quot;text-gray-600 mt-1\\\&quot;\u003eUpload and validate provider data files\u003c/p\u003e\\n      \u003c/div\u003e\\n\\n      {/* Upload Section */}\\n      \u003cdiv className\u003d\\\&quot;bg-white rounded-lg shadow p-6 mb-8\\\&quot;\u003e\\n        \u003ch2 className\u003d\\\&quot;text-xl font-bold text-gray-900 mb-4\\\&quot;\u003eUpload File\u003c/h2\u003e\\n        \\n        \u003cdiv className\u003d\\\&quot;border-2 border-dashed border-gray-300 rounded-lg p-8 text-center\\\&quot;\u003e\\n          \u003cUpload className\u003d\\\&quot;mx-auto text-gray-400 mb-4\\\&quot; size\u003d{48} /\u003e\\n          \\n          {file ? (\\n            \u003cdiv className\u003d\\\&quot;mb-4\\\&quot;\u00