ify-center h-full\\\&quot;\u003e\\n        \u003cdiv className\u003d\\\&quot;text-center\\\&quot;\u003e\\n          \u003cdiv className\u003d\\\&quot;animate-spin rounded-full h-12 w-12 border-b-2 border-primary-500 mx-auto\\\&quot;\u003e\u003c/div\u003e\\n          \u003cp className\u003d\\\&quot;mt-4 text-gray-600\\\&quot;\u003eLoading dashboard...\u003c/p\u003e\\n        \u003c/div\u003e\\n      \u003c/div\u003e\\n    );\\n  }\\n\\n  const statCards \u003d [\\n    {\\n      title: \u0027Total Providers\u0027,\\n      value: stats?.totalProviders || 0,\\n      icon: Users,\\n      color: \u0027bg-blue-500\u0027,\\n    },\\n    {\\n      title: \u0027Active Providers\u0027,\\n      value: stats?.activeProviders || 0,\\n      icon: Activity,\\n      color: \u0027bg-green-500\u0027,\\n    },\\n    {\\n      title: \u0027Avg Quality Score\u0027,\\n      value: `${stats?.averageQualityScore || 0}%`,\\n      icon: TrendingUp,\\n      color: \u0027bg-purple-500\u0027,\\n    },\\n    {\\n      title: \u0027Validation Jobs\u0027,\\n      value: stats?.totalValidationJobs || 0,\\n      icon: FileCheck,\\n      color: \u0027bg-orange-500\u0027,\\n    },\\n  ];\\n\\n  return (\\n    \u003cdiv className\u003d\\\&quot;p-8\\\&quot;\u003e\\n      \u003cdiv className\u003d\\\&quot;mb-8\\\&quot;\u003e\\n        \u003ch1 className\u003d\\\&quot;text-3xl font-bold text-gray-900\\\&quot;\u003eDashboard\u003c/h1\u003e\\n        \u003cp className\u003d\\\&quot;text-gray-600 mt-1\\\&quot;\u003eOverview of your provider data\u003c/p\u003e\\n      \u003c/div\u003e\\n\\n      {/* Stats Cards */}\\n      \u003cdiv className\u003d\\\&quot;grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6 mb-8\\\&quot;\u003e\\n        {statCards.map((stat, index) \u003d\u003e (\\n          \u003cdiv key\u003d{index} className\u003d\\\&quot;bg-white rounded-lg shadow p-6\\\&quot;\u003e\\n            \u003cdiv className\u003d\\\&quot;flex items-center justify-between\\\&quot;\u003e\\n              \u003cdiv\u003e\\n                \u003cp className\u003d\\\&quot;text-sm text-gray-600 mb-1\\\&quot;\u003e{stat.title}\u003c/p\u003e\\n                \u003cp className\u003d\\\&quot;text-3xl font-bold text-gray-900\\\&quot;\u003e{stat.value}\u003c/p\u003e\\n              \u003c/div\u003e\\n              \u003cdiv className\u003d{`${stat.color} p-3 rounded-lg`}\u003e\\n                \u003cstat.icon className\u003d\\\&quot;text-white\\\&quot; size\u003d{24} /\u003e\\n              \u003c/div\u003e\\n            \u003c/div\u003e\\n          \u003c/div\u003e\\n        ))}\\n      \u003c/div\u003e\\n\\n      {/* Quality Distribution */}\\n      {stats?.qualityDistribution \u0026\u0026 stats.qualityDistribution.length \u003e 0 \u0026\u0026 (\\n        \u003cdiv className\u003d\\\&quot;bg-white rounded-lg shadow p-6 mb-8\\\&quot;\u003e\\n          \u003ch2 className\u003d\\\&quot;text-xl font-bold text-gray-900 mb-4\\\&quot;\u003eQuality Score Distribution\u003c/h2\u003e\\n          \u003cdiv className\u003d\\\&quot;space-y-3\\\&quot;\u003e\\n            {stats.qualityDistribution.map((item, index) \u003d\u003e (\\n              \u003cdiv key\u003d{index}\u003e\\n                \u003cdiv className\u003d\\\&quot;flex justify-between text-sm mb-1\\\&quot;\u003e\\n                  \u003cspan className\u003d\\\&quot;text-gray-600\\\&quot;\u003e{item.quality_range}\u003c/span\u003e\\n                  \u003cspan className\u003d\\\&quot;font-medium text-gray-900\\\&quot;\u003e{item.count} providers\u003c/span\u003e\\n                \u003c/div\u003e\\n                \u003cdiv className\u003d\\\&quot;w-full bg-gray-200 rounded-full h-2\\\&quot;\u003e\\n                  \u003cdiv\\n                    className\u003d\\\&quot;bg-primary-500 h-2 rounded-full\\\&quot;\\n                    style\u003d{{\\n                      width: `${(item.count / stats.totalProviders) * 100}%`,\\n                    }}\\n                  /\u003e\\n                \u003c/div\u003e\\n              \u